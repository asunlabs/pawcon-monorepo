{
    "trailingComma": "es5",
    "semi": true,
    "tabWidth": 4,
    "singleQuote": true,
    "printWidth": 150,
    "useTabs": false,
    "arrowParens": "always",
    "bracketSpacing": true,
    "bracketSameLine": false,
    "jsxSingleQuote": false,
    "overrides": [
        {
            "files": "*.sol",
            "options": {
                "printWidth": 150,
                "singleQuote": false,
                "explicitTypes": "always"
            }
        }
    ]
}
